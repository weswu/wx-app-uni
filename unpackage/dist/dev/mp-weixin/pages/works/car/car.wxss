page {
  background-color: #f8f8f8;
}
.get .add-round {
  position: fixed;
  z-index: 999;
  right: 30rpx;
  bottom: 30rpx;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  width: 120rpx;
  height: 120rpx;
  color: #fff;
  text-align: center;
  line-height: 120rpx;
  font-weight: 100;
  font-size: 80rpx;
}
.get .get-list {
  margin-top: 20rpx;
}
.get .get-list .row {
  width: calc(94%);
  height: calc(22vw + 40rpx);
  margin: 0 auto 20rpx;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  -webkit-box-shadow: 0rpx 5rpx 20rpx rgba(0, 0, 0, 0.1);
          box-shadow: 0rpx 5rpx 20rpx rgba(0, 0, 0, 0.1);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
  overflow: hidden;
  z-index: 4;
  border: 0;
}
.get .get-list .row .carrier {
  background-color: #fff;
  position: absolute;
  width: 100%;
  padding: 0 0;
  height: 100%;
  z-index: 3;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
@-webkit-keyframes showMenu {
0% {
    -webkit-transform: translateX(0);
            transform: translateX(0);
}
100% {
    -webkit-transform: translateX(-30%);
            transform: translateX(-30%);
}
}
@keyframes showMenu {
0% {
    -webkit-transform: translateX(0);
            transform: translateX(0);
}
100% {
    -webkit-transform: translateX(-30%);
            transform: translateX(-30%);
}
}
@-webkit-keyframes closeMenu {
0% {
    -webkit-transform: translateX(-30%);
            transform: translateX(-30%);
}
100% {
    -webkit-transform: translateX(0);
            transform: translateX(0);
}
}
@keyframes closeMenu {
0% {
    -webkit-transform: translateX(-30%);
            transform: translateX(-30%);
}
100% {
    -webkit-transform: translateX(0);
            transform: translateX(0);
}
}
.get .get-list .get-wrapper {
  background-color: #fff;
  width: 100%;
  padding: 20rpx 30rpx 20rpx 30rpx;
  margin: 20rpx 0 0;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  position: relative;
}
.get .get-list .get-wrapper .title {
  font-size: 32rpx;
  color: #303133;
  font-weight: 500;
  margin: 0 0 10rpx;
}
.get .get-list .get-wrapper .content {
  font-size: 24rpx;
  color: #606266;
  height: 75rpx;
  line-height: 36rpx;
}
.get .get-list .get-wrapper .time {
  font-size: 28rpx;
  color: #909399;
}

