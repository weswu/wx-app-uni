<view class="page"><view class="input-t "><text>车型</text><view class="tit"><picker value="{{typeindex}}" range="{{type}}" data-event-opts="{{[['change',[['bindTypeChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{type[typeindex]}}</view></picker></view></view><view class="input-t "><text>名称</text><input class="input tit" type="text" placeholder="请输入车辆名称或型号" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','title','$event',[]],['sendDate']]]]]}}" value="{{sendDate.title}}" bindinput="__e"/></view><view class="input-t "><text>车牌号</text><input class="input tit" type="text" placeholder="请输入车牌号" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','plate_number','$event',[]],['sendDate']]]]]}}" value="{{sendDate.plate_number}}" bindinput="__e"/></view><view class="input-t "><text>行驶证</text><input class="input tit" type="text" placeholder="请输入行驶证" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','vehicle_license','$event',[]],['sendDate']]]]]}}" value="{{sendDate.vehicle_license}}" bindinput="__e"/></view><view class="input-t "><text>里程(KM)</text><input class="input tit" type="text" placeholder="请输入里程数" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mileage','$event',[]],['sendDate']]]]]}}" value="{{sendDate.mileage}}" bindinput="__e"/></view><view class="input-t "><text>保险到期</text><picker mode="date" value="{{sendDate.insurance}}" data-event-opts="{{[['change',[['bindInsuranceChange',['$event']]]],['input',[['__set_model',['$0','insurance','$event',[]],['sendDate']]]]]}}" bindchange="__e" bindinput="__e"><view class="date" style="background:none;">{{insurance||'请选择日期'}}</view></picker></view><view class="input-t "><text>年检到期</text><picker mode="date" value="{{sendDate.inspection}}" data-event-opts="{{[['change',[['bindInspectionChange',['$event']]]],['input',[['__set_model',['$0','inspection','$event',[]],['sendDate']]]]]}}" bindchange="__e" bindinput="__e"><view class="date" style="background:none;">{{inspection||'请选择日期'}}</view></picker></view><view class="cu-form-group"><view>状态</view><radio-group data-event-opts="{{[['change',[['handleStateChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{state}}" wx:for-item="item" wx:for-index="index" wx:key="index"><label class="radio margin-right"><radio style="transform:scale(0.75);" value="{{item.key}}" checked="{{item.key===sendDate.state}}" color="{{themeColor.color}}"></radio>{{item.value}}</label></block></radio-group></view><view class="input-t"><text>备注</text></view><view class="input-body"><textarea class="input-textare" placeholder="请输入车辆备注" data-event-opts="{{[['input',[['__set_model',['$0','mark','$event',[]],['sendDate']]]]]}}" value="{{sendDate.mark}}" bindinput="__e"></textarea></view><view class="covers-title"><text>上传图片(请上传图片)</text></view><view class="covers-body covers-uploader oa-uploader"><view class="uni-uploader"><view class="uni-uploader-head"><view class="uni-uploader-title">点击预览图片</view><view class="uni-uploader-info">{{imageList.length+"/6"}}</view></view><view class="uni-uploader-body"><view class="uni-uploader__files"><block wx:for="{{imageList}}" wx:for-item="image" wx:for-index="index" wx:key="index"><block><view class="uni-uploader__file" style="position:relative;"><oa-image class="uni-uploader__img" vue-id="{{'0dc95ad2-1-'+index}}" src="{{image}}" bind:__l="__l"></oa-image><view data-event-opts="{{[['tap',[['close',[index]]]]]}}" class="{{['close-view','bg-'+themeColor.name]}}" bindtap="__e">x</view></view></block></block><block wx:if="{{imageList.length<6}}"><view class="uni-uploader__input-box"><view data-event-opts="{{[['tap',[['uploadImage',['$event']]]]]}}" class="uni-uploader__input" bindtap="__e"></view></view></block></view></view></view></view><button class="{{['confirm-btn','bg-'+themeColor.name]}}" disabled="{{btnLoading}}" loading="{{btnLoading}}" data-event-opts="{{[['tap',[['send',['$event']]]]]}}" bindtap="__e">确定提交</button></view>