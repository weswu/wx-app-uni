<view class="page"><view class="input-t "><text>编号</text><input class="input tit" type="text" placeholder="请输入员工编号,不能为空" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','serial','$event',[]],['sendDate']]]]]}}" value="{{sendDate.serial}}" bindinput="__e"/></view><view class="input-t "><text>姓名</text><input class="input tit" type="text" placeholder="请输入真实姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['sendDate']]]]]}}" value="{{sendDate.name}}" bindinput="__e"/></view><view class="cu-form-group"><view>性别</view><radio-group data-event-opts="{{[['change',[['handleGenderChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{gender}}" wx:for-item="item" wx:for-index="index" wx:key="index"><label class="radio margin-right"><radio style="transform:scale(0.75);" value="{{item.key}}" checked="{{item.key===sendDate.gender}}" color="{{themeColor.color}}"></radio>{{''+item.value+''}}</label></block></radio-group></view><view class="input-t "><text>身份证</text><input class="input tit" type="text" placeholder="请输入18位身份证号" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','idcard','$event',[]],['sendDate']]]]]}}" value="{{sendDate.idcard}}" bindinput="__e"/></view><view class="input-t "><text>政治面貌</text><view class="tit"><picker value="{{typeindex}}" range="{{type}}" data-event-opts="{{[['change',[['bindTypeChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{type[typeindex]}}</view></picker></view></view><view class="input-t "><text>籍贯</text><input class="input tit" type="text" placeholder="请输入籍贯" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','native','$event',[]],['sendDate']]]]]}}" value="{{sendDate.native}}" bindinput="__e"/></view><view class="input-t "><text>学历</text><input class="input tit" type="text" placeholder="请输入最高学历" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','education','$event',[]],['sendDate']]]]]}}" value="{{sendDate.education}}" bindinput="__e"/></view><view class="input-t "><text>毕业院校</text><input class="input tit" type="text" placeholder="请输入毕业院校" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','colleges','$event',[]],['sendDate']]]]]}}" value="{{sendDate.colleges}}" bindinput="__e"/></view><view class="input-t "><text>户口所在地</text><input class="input tit" type="text" placeholder="请输入户口所在地" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','residence','$event',[]],['sendDate']]]]]}}" value="{{sendDate.residence}}" bindinput="__e"/></view><view class="input-t "><text>手机</text><input class="input tit" type="text" placeholder="请输入手机号" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['sendDate']]]]]}}" value="{{sendDate.mobile}}" bindinput="__e"/></view><view class="input-t "><text>邮箱</text><input class="input tit" type="text" placeholder="请输入电子邮箱" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mail','$event',[]],['sendDate']]]]]}}" value="{{sendDate.mail}}" bindinput="__e"/></view><button class="{{['confirm-btn','bg-'+themeColor.name]}}" disabled="{{btnLoading}}" loading="{{btnLoading}}" data-event-opts="{{[['tap',[['send',['$event']]]]]}}" bindtap="__e">确定提交</button></view>