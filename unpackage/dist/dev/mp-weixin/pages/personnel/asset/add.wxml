<view class="page"><view class="input-t "><text>类型</text><view class="tit"><picker value="{{typeindex}}" range="{{type}}" data-event-opts="{{[['change',[['bindTypeChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input">{{type[typeindex]}}</view></picker></view></view><view class="input-t "><text>编号</text><input class="input tit" type="text" placeholder="请输入资产编号,不能为空" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','serial','$event',[]],['sendDate']]]]]}}" value="{{sendDate.serial}}" bindinput="__e"/></view><view class="input-t "><text>名称</text><input class="input tit" type="text" placeholder="请输入资产名称,不要超过25个字符" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','title','$event',[]],['sendDate']]]]]}}" value="{{sendDate.title}}" bindinput="__e"/></view><view class="input-t "><text>单价</text><input class="input tit" type="text" placeholder="请输入单价" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','purchase_price','$event',[]],['sendDate']]]]]}}" value="{{sendDate.purchase_price}}" bindinput="__e"/></view><view class="input-t "><text>数量</text><input class="input tit" type="text" placeholder="请输入整数" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','num','$event',[]],['sendDate']]]]]}}" value="{{sendDate.num}}" bindinput="__e"/></view><view class="input-t "><text>单位</text><input class="input tit" type="text" placeholder="请输入资产的单位,比如:台" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','unit','$event',[]],['sendDate']]]]]}}" value="{{sendDate.unit}}" bindinput="__e"/></view><view class="input-t "><text>部门</text><input class="input tit" type="text" placeholder="请输入所属部门" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','department','$event',[]],['sendDate']]]]]}}" value="{{sendDate.department}}" bindinput="__e"/></view><view class="input-t "><text>地点</text><input class="input tit" type="text" placeholder="请输入所在地点" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['sendDate']]]]]}}" value="{{sendDate.address}}" bindinput="__e"/></view><view class="input-t "><text>负责人</text><input class="input tit" type="text" placeholder="请输入资产负责人" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','person','$event',[]],['sendDate']]]]]}}" value="{{sendDate.person}}" bindinput="__e"/></view><view class="input-t "><text>购买日期</text><picker mode="date" value="{{sendDate.purchase_date}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]],['input',[['__set_model',['$0','purchase_date','$event',[]],['sendDate']]]]]}}" bindchange="__e" bindinput="__e"><view class="date" style="background:none;">{{purchase_date||'请选择日期'}}</view></picker></view><button class="{{['confirm-btn','bg-'+themeColor.name]}}" disabled="{{btnLoading}}" loading="{{btnLoading}}" data-event-opts="{{[['tap',[['send',['$event']]]]]}}" bindtap="__e">确定提交</button></view>